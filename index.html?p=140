<script>
    class TabList {
        constructor(buttonsContainer, tabs) {
        this.buttonsContainer = buttonsContainer;
        this.tabs = tabs;
        this.buttonsContainer.addEventListener('click', (event) => {
          const btn = event.target.closest('a');
          const index = btn.dataset.tab;

          if (index) {
            this.openTab(index, btn);
          }
        });
      }

      openTab(index, btn) {
        this.tabs.querySelector('.active')?.classList.remove('active');
        this.tabs.querySelector(`[data-tab="${index}"]`)?.classList.add('active');

        this.buttonsContainer.querySelector('.active')?.classList.remove('active');
        btn.classList.add('active');
        }
    }
    document.addEventListener('DOMContentLoaded', () => {
        const buttonsContainer = document.querySelector('.js-tabs-btns');
        const tabs = document.querySelector('.js-tabs-items');

      if (buttonsContainer && tabs) {
        const tabList = new TabList(buttonsContainer, tabs);
      }

        const buttonsWhoisContainer = document.querySelector('.js-tabs-whois-btns');
        const tabsWhois = document.querySelector('.js-tabs-whois-items');

      if (buttonsWhoisContainer && tabsWhois) {
        const tabList = new TabList(buttonsWhoisContainer, tabsWhois);
      }

        const buttonsPortfolioContainer = document.querySelector('.js-tabs-portfolio-btns');
        const tabsPortfolio = document.querySelector('.js-tabs-portfolio-items');

      if (buttonsPortfolioContainer && tabsPortfolio) {
        const tabList = new TabList(buttonsPortfolioContainer, tabsPortfolio);
      }
    });

    const selectTabs = document.querySelector('.js-tabs-select');
    const buttonsContainer = document.querySelector('.js-tabs-portfolio-btns');
    const tabs = document.querySelector('.js-tabs-portfolio-items');

    if (selectTabs) {
      selectTabs.addEventListener('change', (e) => {
        const index = e.target.value;
        tabs.querySelector('.active')?.classList.remove('active');
        tabs.querySelector(`[data-tab="${index}"]`)?.classList.add('active');
      });
    }


            var portfolioLocation = window.location.hash;

            if (portfolioLocation == '#1')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="1"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('1').classList.add('active');
            }

            if (portfolioLocation == '#2')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="2"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('2').classList.add('active');
            }

            if (portfolioLocation == '#3')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="3"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('3').classList.add('active');
            }

            if (portfolioLocation == '#4')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="4"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('4').classList.add('active');
            }

            if (portfolioLocation == '#5')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="5"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('5').classList.add('active');
            }

            if (portfolioLocation == '#6')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="6"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('6').classList.add('active');
            }

            if (portfolioLocation == '#7')  {
                document.querySelector('[data-tab="0"]').classList.remove('active');
                document.querySelector('[data-tab="7"]').classList.add('active');
                document.getElementById('0').classList.remove('active');
                document.getElementById('7').classList.add('active');
            }
</script>

